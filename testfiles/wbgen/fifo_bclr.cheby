memory-map:
  name: fifo
  comment: FIFO bclr
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifoclr
  schema-version:
    core: 2.0.0
    x-conversions: 1.0.0
    x-driver-edge: 1.0.0
    x-enums: 1.0.0
    x-fesa: 2.0.0
    x-gena: 2.0.0
    x-hdl: 1.0.0
    x-map-info: 1.0.0
    x-wbgen: 1.0.0
  children:
  - block:
      name: fifo_bclr
      comment: fifo bus clear
      address: 0x0
      align: False
      size: 8
      x-wbgen:
