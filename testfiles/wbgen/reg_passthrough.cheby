memory-map:
  name: pt
  comment: register passthrough
  description: A simple passthrough register
  bus: wb-32-be
  x-wbgen:
    hdl_entity: pt
  schema-version:
    core: 2.0.0
    x-conversions: 1.0.0
    x-driver-edge: 1.0.0
    x-enums: 1.0.0
    x-fesa: 2.0.0
    x-gena: 2.0.0
    x-hdl: 1.0.0
    x-map-info: 1.0.0
    x-wbgen: 1.0.0
  children:
  - reg:
      name: R1
      comment: Register 1
      description: Register 1 has two fields
      width: 32
      access: wo
      address: 0x0
      x-hdl:
        write-strobe: True
      children:
      - field:
          name: reset
          comment: Reset bit
          description: 1 to reset
          range: 0
          x-wbgen:
            access_bus: WRITE_ONLY
            access_dev: READ_ONLY
            size: 1
            type: PASS_THROUGH
          x-hdl:
            type: wire
  - reg:
      name: R2
      comment: Register 2
      description: Register 2
      width: 32
      access: wo
      address: 0x4
      x-hdl:
        write-strobe: True
      children:
      - field:
          name: reset
          comment: Reset bit
          description: 1 to reset
          range: 0
          x-wbgen:
            access_bus: WRITE_ONLY
            access_dev: READ_ONLY
            size: 1
            type: PASS_THROUGH
          x-hdl:
            type: wire
