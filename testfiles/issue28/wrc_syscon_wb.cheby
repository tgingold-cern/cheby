memory-map:
  name: sysc
  comment: WR Core System Controller
  bus: wb-32-be
  x-wbgen:
    hdl_entity: wrc_syscon_wb
  schema-version:
    core: 2.0.0
    x-conversions: 1.0.0
    x-driver-edge: 1.0.0
    x-enums: 1.0.0
    x-fesa: 2.0.0
    x-gena: 2.0.0
    x-hdl: 1.0.0
    x-map-info: 1.0.0
    x-wbgen: 1.0.0
  children:
  - reg:
      name: RSTR
      comment: Syscon reset register
      width: 32
      access: rw
      address: 0x0
      x-hdl:
        write-strobe: True
      children:
      - field:
          name: TRIG
          comment: Reset trigger
          description: Write 0xdeadbee latch the state of RSTR.RST to the reset line of the LM32 CPU.
          range: 27-0
          x-wbgen:
            type: PASS_THROUGH
          x-hdl:
            type: wire
      - field:
          name: RST
          comment: Reset line state value
          description: State of the reset line
          range: 28
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
  - reg:
      name: GPSR
      comment: GPIO Set/Readback Register
      width: 32
      access: rw
      address: 0x4
      x-hdl:
        write-strobe: True
      children:
      - field:
          name: led_stat
          comment: Status LED
          description: 'Write 1: turn on the Status LED'
          range: 0
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: led_link
          comment: Link LED
          description: 'Write 1: turn on the Link LED'
          range: 1
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: fmc_scl
          comment: FMC I2C bitbanged SCL
          description: 'write 1: Set FMC SCL line to 1 (pullup)\n                        read : returns the current status of the SCL line.'
          range: 2
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: fmc_sda
          comment: FMC I2C bitbanged SDA
          description: 'write 1: Set FMC SDA line to 1 (pullup)\n                        read : returns the current status of the SCL line.'
          range: 3
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: Net_RST
          comment: Network AP reset
          description: 'write 1: resets the networking subsystem'
          range: 4
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: Btn1
          comment: SPEC Pushbutton 1 state
          description: 'read : returns the state of the Button 1 on the SPEC board'
          range: 5
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: BIT
      - field:
          name: Btn2
          comment: SPEC Pushbutton 2 state
          description: 'read : returns the state of the Button 2 on the SPEC board'
          range: 6
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: BIT
      - field:
          name: sfp_det
          comment: SFP detect (MOD_DEF0 signal)
          description: 'read : returns the state of the SFP''s MOD_DEF0 line'
          range: 7
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: BIT
      - field:
          name: sfp_scl
          comment: SFP I2C bitbanged SCL
          description: 'write 1: Set SFP SCL line to 1 (pullup)\n                        read : returns the current status of the SCL line.'
          range: 8
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: sfp_sda
          comment: SFP I2C bitbanged SDA
          description: 'write 1: Set SFP SDA line to 1 (pullup)\n                        read : returns the current status of the SCL line.'
          range: 9
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: spi_sclk
          comment: SPI bitbanged SCLK
          description: 'write 1: drive SPI CLK to 1\n                       read: always 0'
          range: 10
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: spi_ncs
          comment: SPI bitbanged NCS
          description: 'write 1: drive SPI NCS to 1\n                       read: always 0'
          range: 11
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: spi_mosi
          comment: SPI bitbanged MOSI
          description: 'write 1: drive SPI MOSI line to 1\n                       read: always 0'
          range: 12
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: BIT
      - field:
          name: spi_miso
          comment: SPI bitbanged MISO
          description: 'read: current state of SPI MISO line'
          range: 13
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            load: LOAD_EXT
            type: BIT
  - reg:
      name: GPCR
      comment: GPIO Clear Register
      width: 32
      access: wo
      address: 0x8
      children:
      - field:
          name: led_stat
          comment: Status LED
          description: 'Write 1: turn on the Status LED'
          range: 0
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: led_link
          comment: Link LED
          description: 'Write 1: turn on the Link LED'
          range: 1
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: fmc_scl
          comment: FMC I2C bitbanged SCL
          description: 'write 1: Set FMC SCL line to 1 (pullup)\n                        read : returns the current status of the SCL line.'
          range: 2
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: fmc_sda
          comment: FMC I2C bitbanged SDA
          description: 'write 1: Set FMC SDA line to 1 (pullup)\n                        read : returns the current status of the SCL line.'
          range: 3
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: sfp_scl
          comment: SFP I2C bitbanged SCL
          description: 'write 1: Set SFP SCL line to 0.'
          range: 8
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: sfp_sda
          comment: FMC I2C bitbanged SDA
          description: 'write 1: Set SFP SDA line to 0.'
          range: 9
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: spi_sclk
          comment: SPI bitbanged SCLK
          description: 'write 1: Set SPI CLK line to 0.'
          range: 10
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: spi_cs
          comment: SPI bitbanged CS
          description: 'write 1: Set SPI CS line to 0'
          range: 11
          x-wbgen:
            type: MONOSTABLE
      - field:
          name: spi_mosi
          comment: SPI bitbanged MOSI
          description: 'write 1: Set SPI MOSI line to 0.'
          range: 12
          x-wbgen:
            type: MONOSTABLE
  - reg:
      name: HWFR
      comment: Hardware Feature Register
      width: 32
      access: ro
      address: 0xc
      children:
      - field:
          name: MEMSIZE
          comment: Memory size
          description: 'Size of installed memory: 0 = 64 kB ... 0xf = 1024 kB'
          range: 3-0
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
  - reg:
      name: HWIR
      comment: Hardware Info Register
      width: 32
      access: ro
      address: 0x10
      children:
      - field:
          name: NAME
          comment: Board name
          description: 4-letter name of the board
          range: 31-0
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
  - reg:
      name: TCR
      comment: Timer Control Register
      width: 32
      access: rw
      address: 0x14
      children:
      - field:
          name: TDIV
          comment: Timer Divider
          description: 'Division for obtain 1 millisecond ticks from the system clock: \n                        <b>f_tick = f_clk_sys / (TDIV * 256)</b>'
          range: 11-0
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
      - field:
          name: ENABLE
          comment: Timer Enable
          description: 'write 1: enable timer\n                        write 0: disable timer'
          range: 31
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
  - reg:
      name: TVR
      comment: Timer Counter Value Register
      width: 32
      access: ro
      address: 0x18
      x-wbgen:
        access_bus: READ_ONLY
        access_dev: WRITE_ONLY
        field_comment: Timer Counter Value
        type: SLV
  - reg:
      name: DIAG_INFO
      comment: 'User Diag: version register'
      width: 32
      access: ro
      address: 0x1c
      children:
      - field:
          name: VER
          comment: Ver
          range: 15-0
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
      - field:
          name: ID
          comment: Id
          range: 31-16
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
  - reg:
      name: DIAG_NW
      comment: 'User Diag: number of words'
      width: 32
      access: ro
      address: 0x20
      children:
      - field:
          name: RW
          comment: Read/write words
          range: 15-0
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
      - field:
          name: RO
          comment: Read-only words
          range: 31-16
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: SLV
  - reg:
      name: DIAG_CR
      comment: 'User Diag: Control Register'
      width: 32
      access: rw
      address: 0x24
      x-hdl:
        write-strobe: True
      children:
      - field:
          name: ADR
          comment: Address
          range: 15-0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_WRITE
            load: LOAD_EXT
            type: SLV
      - field:
          name: RW
          comment: R/W
          range: 31
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            size: 1
            type: BIT
  - reg:
      name: DIAG_DAT
      comment: 'User Diag: data to read/write'
      width: 32
      access: rw
      address: 0x28
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_WRITE
        field_comment: Data
        load: LOAD_EXT
        type: SLV
      x-hdl:
        write-strobe: True
  - reg:
      name: WDIAG_CTRL
      comment: 'WRPC Diag: ctrl'
      width: 32
      access: rw
      address: 0x2c
      children:
      - field:
          name: DATA_VALID
          comment: WR DIAG data valid
          description: '0: valid\n                     1: transcient'
          range: 0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
      - field:
          name: DATA_SNAPSHOT
          comment: WR DIAG data snapshot
          description: '1: snapshot data (data in registers will not change aveter VALID becomes true)'
          range: 8
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            type: BIT
  - reg:
      name: WDIAG_SSTAT
      comment: 'WRPC Diag: servo status'
      width: 32
      access: rw
      address: 0x30
      children:
      - field:
          name: wr_mode
          comment: WR valid
          description: '0: not valid\1:valid'
          range: 0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
      - field:
          name: servostate
          comment: Servo State
          description: '0: Uninitialized\n                     1: SYNC_NSEC\n                     2: SYNC_TAI\n                     3: SYNC_PHASE\n                     4: TRACK_PHASE\n                     5: WAIT_OFFSET_STABLE'
          range: 11-8
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: SLV
  - reg:
      name: WDIAG_PSTAT
      comment: 'WRPC Diag: Port status'
      width: 32
      access: rw
      address: 0x34
      children:
      - field:
          name: link
          comment: Link Status
          description: '0: link down\n                     1: link up'
          range: 0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
      - field:
          name: locked
          comment: PLL Locked
          description: '0: not locked\n                     1: locked'
          range: 1
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
  - reg:
      name: WDIAG_PTPSTAT
      comment: 'WRPC Diag: PTP state'
      width: 32
      access: rw
      address: 0x38
      children:
      - field:
          name: ptpstate
          comment: PTP State
          description: '0: NONE\n                     1: PPS_INITIALIZING\n                     2: PPS_FAULTY\n                     3: disabled\n                     4: PPS_LISTENING\n                     5: PPS_PRE_MASTER\n                     6: PPS_MASTER\n                     7: PPS_PASSIVE\n                     8: PPS_UNCALIBRATED\n                     9: PPS_SLAVE\n                     100-116: WR STATES\n                     see: ppsi/proto-ext-whiterabbit/wr-constants.h\n                          ppsi/include/ppsi/ieee1588_types.h'
          range: 7-0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: SLV
  - reg:
      name: WDIAG_ASTAT
      comment: 'WRPC Diag: AUX state'
      width: 32
      access: rw
      address: 0x3c
      children:
      - field:
          name: aux
          comment: AUX channel
          description: 'A vector of bits, one bit per channel\n                     0: not valid\n                     1:valid'
          range: 7-0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: SLV
  - reg:
      name: WDIAG_TXFCNT
      comment: 'WRPC Diag: Tx PTP Frame cnts'
      description: Number of transmitted PTP Frames
      width: 32
      access: rw
      address: 0x40
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_RXFCNT
      comment: 'WRPC Diag: Rx PTP Frame cnts'
      description: Number of received PTP Frames
      width: 32
      access: rw
      address: 0x44
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_SEC_MSB
      comment: 'WRPC Diag:local time [msb of s]'
      description: Local Time expressed in seconds since epoch (TAI)
      width: 32
      access: rw
      address: 0x48
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_SEC_LSB
      comment: 'WRPC Diag: local time [lsb of s]'
      description: Local Time expressed in seconds since epoch (TAI)
      width: 32
      access: rw
      address: 0x4c
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_NS
      comment: 'WRPC Diag: local time [ns]'
      description: Nanoseconds part of the Local Time expressed in seconds since epoch (TAI)
      width: 32
      access: rw
      address: 0x50
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_MU_MSB
      comment: 'WRPC Diag: Round trip (mu) [msb of ps]'
      width: 32
      access: rw
      address: 0x54
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_MU_LSB
      comment: 'WRPC Diag: Round trip (mu) [lsb of ps]'
      width: 32
      access: rw
      address: 0x58
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_DMS_MSB
      comment: 'WRPC Diag: Master-slave delay (dms) [msb of ps]'
      width: 32
      access: rw
      address: 0x5c
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_DMS_LSB
      comment: 'WRPC Diag: Master-slave delay (dms) [lsb of ps]'
      width: 32
      access: rw
      address: 0x60
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_ASYM
      comment: 'WRPC Diag: Total link asymmetry [ps]'
      width: 32
      access: rw
      address: 0x64
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_CKO
      comment: 'WRPC Diag: Clock offset (cko) [ps]'
      width: 32
      access: rw
      address: 0x68
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_SETP
      comment: 'WRPC Diag: Phase setpoint (setp) [ps]'
      width: 32
      access: rw
      address: 0x6c
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_UCNT
      comment: 'WRPC Diag: Update counter (ucnt)'
      width: 32
      access: rw
      address: 0x70
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
  - reg:
      name: WDIAG_TEMP
      comment: 'WRPC Diag: Board temperature [C degree]'
      width: 32
      access: rw
      address: 0x74
      x-wbgen:
        access_bus: READ_WRITE
        access_dev: READ_ONLY
        field_comment: Data
        type: SLV
